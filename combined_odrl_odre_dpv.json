      "rdfs:label": {
        "@value": "Enforcement Framework",
        "@language": "en"
      },
      "rdfs:comment": {
        "@value": "A framework for enforcing digital rights and privacy policies",
        "@language": "en"
      },
      "skos:definition": {
        "@value": "Technical and organizational infrastructure for monitoring and enforcing policy compliance",
        "@language": "en"
      }
    },
    {
