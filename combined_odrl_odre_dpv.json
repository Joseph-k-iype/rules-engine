      "dcterms:description": {
        "@value": "Enforceable policy based on GDPR Article 28 processor requirements",
        "@language": "en"
      },
      "odre:enforceable": {
        "@value": true,
        "@type": "xsd:boolean"
      },
      "odre:enforcementMode": "real_time",
      "odre:monitoringRequired": {
        "@value": true,
        "@type": "xsd:boolean"
      },
      "odre:complianceCheck": {
        "@value": true,
        "@type": "xsd:boolean"
      },
      "odre:temporalEnforcement": {
        "@value": false,
        "@type": "xsd:boolean"
