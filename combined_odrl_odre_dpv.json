  "@graph": [
    {
      "@id": "https://w3id.org/def/integrated-dpv-odrl-odre",
      "@type": "owl:Ontology",
      "dcterms:title": {
        "@value": "Integrated DPV+ODRL+ODRE Ontology",
        "@language": "en"
      },
      "dcterms:description": {
        "@value": "A combined ontology integrating Data Privacy Vocabulary (DPV), Open Digital Rights Language (ODRL), and Open Digital Rights Enforcement (ODRE) for comprehensive privacy rights management and enforcement.",
        "@language": "en"
      },
      "dcterms:created": {
        "@value": "2025-01-01",
        "@type": "xsd:date"
      },
      "dcterms:creator": "Legislation Rules Converter System",
